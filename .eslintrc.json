{
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	// "extends": ["eslint:recommended", "plugin:react/recommended"],
	"extends": [
		"airbnb",
		"airbnb/hooks"
		// "plugin:prettier/recommended",
		// "prettier/react"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"parser": "babel-eslint",
	"rules": {
		"no-console": "warn"
		// "prettier/prettier": "warn"
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	// "plugins": ["react", "prettier"]
	"plugins": ["react"]
}
